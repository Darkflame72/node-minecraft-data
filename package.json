{
  "name": "minecraft-data",
  "version": "2.70.2",
  "description": "Provide easy access to minecraft data in node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "tonicExampleFilename": "example.js",
  "scripts": {
    "generate:data": "./bin/generate_data.js",
    "test": "npm run generate:data && npm run lint && mocha",
    "lint": "ts-standard",
    "format": "ts-standard --fix",
    "build": "tsc",
    "prepublishOnly": "npm test && npm run lint",
    "prepare": "npm run build && npm run generate:data",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "standard": {
    "ignore": [
      "minecraft-data/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:PrismarineJS/node-minecraft-data.git"
  },
  "keywords": [
    "minecraft",
    "data",
    "node.js"
  ],
  "author": "Romain Beaumont <romain.rom1@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PrismarineJS/node-minecraft-data/issues"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "mocha": "^8.0.1",
    "quicktype": "^15.0.258",
    "ts-standard": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}